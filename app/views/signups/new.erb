<main> 
<%= form_for @signup do |f| %>
    <%= f.label :time, "time of activity(hour)" %>
    <%= f.number_field :time %><br><br>
    <%= f.label :camper_id %>
    <%= f.collection_select :camper_id, @campers, :id, :name %><br><br>
    <%= f.label :activity_id %>
    <%= f.collection_select :activity_id, @activities, :id, :name %><br><br>
    <%= f.submit %>
  <% end %>
</main>

